{
  "version": 1,
  "framework": {
    "name": "nextjs",
    "version": "15.3.1"
  },
  "routes": [
    {
      "path": "/*",
      "target": {
        "kind": "Compute",
        "src": "apps"
      }
    }
  ],
  "computeResources": [
    {
      "name": "apps",
      "runtime": "nodejs20.x",
      "entrypoint": "studio/server.js"
    }
  ]
}
