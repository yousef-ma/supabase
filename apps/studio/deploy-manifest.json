{
  "version": 1,
  "routes": [
    {
      "path": "/_next/static/*",
      "target": "static",
      "exact": false
    },
    {
      "path": "/static/*",
      "target": "static",
      "exact": false
    },
    {
      "path": "/*.js",
      "target": "static",
      "exact": false
    },
    {
      "path": "/*.json",
      "target": "static",
      "exact": false
    },
    {
      "path": "/favicon.ico",
      "target": "static",
      "exact": true
    },
    {
      "path": "/*",
      "target": "server",
      "exact": false,
      "rewrite": {
        "source": "/*",
        "destination": "/apps/studio/server.js"
      }
    }
  ],
  "server": {
    "runtime": "nodejs20.x",
    "handler": "server.js",
    "launcherType": "Nextjs",
    "regions": ["us-east-1"]
  }
}
