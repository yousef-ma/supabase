{
  "version": 1,
  "routes": [
    {
      "path": "/*",
      "ssr": true,
      "data": {
        "enableStaticRewrites": true
      },
      "rewrite": {
        "source": "/*",
        "destination": "/apps/studio/server.js"
      }
    },
    {
      "path": "/_next/static/*",
      "static": true
    },
    {
      "path": "/*.*",
      "status": 200,
      "target": "static"
    }
  ]
}
