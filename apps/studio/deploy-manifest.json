{
  "version": 1,
  "framework": {
      "name": "nextjs",
      "version": "15.3.1"
    },
  "routes": [
    {
      "path": "/*",
      "ssr": true,
      "rewrite": {
        "source": "/*",
        "destination": "/apps/studio/server.js"
      }
    },
    {
      "path": "/_next/static/*",
      "static": true
    }
  ]
}
