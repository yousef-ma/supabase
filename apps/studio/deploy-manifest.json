{
  "version": 1,
  "framework": {
    "name": "nextjs",
    "version": "15.3.1"
  },
  "routes": [
    {
      "path": "/_next/static/*",
      "target": "static"
    },
    {
      "path": "/static/*",
      "target": "static"
    },
    {
      "path": "/favicon.ico",
      "target": "static"
    },
    {
      "path": "/*",
      "target": "server",
      "rewrite": "/apps/studio/server.js"
    }
  ],
  "server": {
    "runtime": "nodejs20.x",
    "handler": "server.js",
    "launcherType": "Nextjs",
    "regions": ["us-east-1"],
    "environment": {}
  }
}
